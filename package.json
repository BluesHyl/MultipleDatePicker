  "repository": {
    "type": "git",
    "url": "https://github.com/BluesHyl/MultipleDatePickerr"
  },
  "bugs": {
    "url": "https://github.com/BluesHyl/MultipleDatePicker/issues"
  },
  "homepage": "https://github.com/BluesHyl/MultipleDatePicker#readme"
